generator client {
  provider        = "prisma-client"
  output          = "../src/app/prisma/generated/ingress"
  previewFeatures = ["typedSql"]
}

datasource db {
  provider  = "postgresql"
  schemas   = ["public"]
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model gamelog {
  id         BigInt                    @id @default(autoincrement())
  event_time DateTime                  @db.Timestamptz(6)
  action     String
  comment    String?
  location   Unsupported("geography")?

  @@index([action, event_time], map: "idx_gamelog_action_time")
  @@index([location], map: "idx_gamelog_location", type: Gist)
  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model store_purchases {
  id                      String   @id
  time                    DateTime @db.Timestamptz(6)
  transaction_type        String
  item                    String
  new_cmu_balance         String
  transaction_description String

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model mind_units_controlled {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@index([time], map: "idx_mind_units_time")
  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model overclock_glyph_hack_points {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model portal_held_days {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model xm_recharged {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model beacon_battles {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model anomaly {
  id          String                   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  timezone    String
  series_name String
  site        String
  start_time  DateTime                 @db.Timestamptz(6)
  end_time    DateTime                 @db.Timestamptz(6)
  location    Unsupported("geography")
  cover_photo String

  @@schema("public")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)

  @@schema("public")
}

model portal_history {
  id       BigInt                   @id
  type     String
  location Unsupported("geography")

  @@index([location], map: "idx_portal_history_geom", type: Gist)
  @@schema("public")
}

model distance_walked {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@schema("public")
}

model medals {
  id            BigInt   @id @default(autoincrement())
  attained_at   DateTime @db.Timestamptz(6)
  medal_name    String
  is_vanity     Boolean
  is_year_badge Boolean

  @@schema("public")
}

model mods_destroyed {
  id    BigInt   @id @default(autoincrement())
  time  DateTime @db.Timestamptz(6)
  value Decimal  @db.Decimal

  @@schema("public")
}
